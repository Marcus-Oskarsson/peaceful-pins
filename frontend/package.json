{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "check-types": "npx tsc --noemit",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0; npm run check-types",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" && stylelint \"src/**/*.{css,scss}\" --fix",
    "preview": "vite preview --port 5173",
    "test": "npx cypress run",
    "test:watch": "npx cypress run --headless --component; npx cypress run --headless --e2e",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:run": "npx cypress run",
    "cy:e2e": "start-server-and-test dev http-get://localhost:3000 cy:open-e2e"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.10.0",
    "axios": "^1.6.2",
    "formik": "^2.4.5",
    "hamburger-react": "^2.5.0",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.20.0",
    "react-toastify": "^9.1.3",
    "usehooks-ts": "^2.9.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@cypress/code-coverage": "^3.12.17",
    "@cypress/react18": "^2.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/cypress": "^1.1.3",
    "@types/leaflet": "^1.9.8",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "cypress": "^13.6.0",
    "eslint": "^8.53.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "mocha": "^10.2.0",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.3.2",
    "vite": "^4.5.1",
    "vite-plugin-istanbul": "^5.0.0",
    "vite-plugin-mkcert": "^1.17.1"
  }
}
